<?cs

string processOutput()
{
    int b = 0;
    
    if(gvarExists("@blah"))
        b = getGVar<int>("@blah");
    
    int @blah = setGVar<int>("@blah", b+1);
    
    string s = "<br><br>HTTP-Request type: " + packet.type + "<br><br>";
    
    for(int i = 0; i < packet.additionalHEAD.Count; i++)
    {
        s += "[HEAD] " + packet.additionalHEAD[i] + " is " + packet.valuesHEAD[i] + "<br>";
    }
    
    for(int i = 0; i < packet.additionalPOST.Count; i++)
    {
        s += "[POST] " + packet.additionalPOST[i] + " is " + packet.valuesPOST[i] + "<br>";
    }
    
	return RemoteIP + s + "<h1>HELLO WORLD, I'M A SCRIPT! [" + @blah + "]</h1><br>" + packet.getPackage().Replace("\r\n","<br>");
}

processOutput()

?>

