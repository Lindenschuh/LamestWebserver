<html><head><title>Overview</title></head><body style='background-color: #f0f0f2;margin: 0;padding: 0;'><div style='font-family: sans-serif;padding: 50px;margin: 2.5%;margin-left: 10%;margin-right: 10%;background-color: #fff;border-radius: 1em;'><?cs

string processOutput()
{
    int? count = getFileVariable< int? >("count");

	if(count != null)
	{
		count++;
		
		setFileVariable(nameof(count), count);
	}
	else
	{
		count = 0;
		
		setFileVariable(nameof(count), count);
	}
    
	return "<h1>HELLO WORLD, I'M A SCRIPT! [" + count.Value + "]</h1><br>" + _rawPacket.Replace("\r\n","<br>");
}

processOutput()

?>

<p style='text-align:right'>- LamestWebserver (LameOS)</p></div></body></html>
